{
  "name": "vmello-website",
  "description": "Source code for Vitor Mello's personal website, built with Next.js and React",
  "author": "Vitor Mello",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=v14.17.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "build": "next build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --cache --cache-location node_modules/.cache/eslint/ .",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:css:fix": "stylelint --fix \"src/**/*.scss\"",
    "export": "npm run build && next export",
    "resume": "npm run export && concurrently --raw --kill-others --success first npm:serve npm:resume:export",
    "resume:export": "node --trace-warnings -r esm cli/export-pdf-resume.js",
    "start": "next dev -p 8091",
    "serve": "node -r esm static.server.js",
    "test": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "next": "^12.0.3",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "puppeteer": "^10.4.0",
    "react": "^18.0.0-alpha-ee069065d-20211105",
    "react-dom": "^18.0.0-alpha-ee069065d-20211105",
    "react-helmet": "^6.1.0",
    "remark": "^14.0.1",
    "remark-html": "^15.0.0",
    "tailwindcss": "^2.2.19",
    "tasuku": "^1.0.2",
    "typeit": "^8.0.7",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.1",
    "@types/google.analytics": "0.0.42",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-helmet": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-next": "^12.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "esm": "^3.2.25",
    "js-yaml-loader": "^1.2.2",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "typescript": "^4.4.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "resolutions": {
    "sass": "1.33.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitormv/vmello-website"
  },
  "bugs": {
    "url": "https://github.com/vitormv/vmello-website/issues"
  }
}
