{
  "name": "vmello-website",
  "description": "Source code for Vitor Mello's personal website, built with Next.js and React",
  "author": "Vitor Mello",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=v16.0.0 <=v17.0.0"
  },
  "scripts": {
    "build": "next build",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --cache --cache-location node_modules/.cache/eslint/ .",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:css:fix": "stylelint --fix \"src/**/*.scss\"",
    "export": "yarn build && next export",
    "resume": "yarn export && concurrently --raw --kill-others --success first npm:serve npm:resume:export",
    "resume:export": "node --trace-warnings -r esm cli/export-pdf-resume.js",
    "start": "next dev -p 8091",
    "start:prod": "next start -p 8091",
    "serve": "node -r esm static.server.js",
    "test": "yarn lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "clsx": "^1.1.1",
    "next": "^13.4.12",
    "normalize.css": "^8.0.1",
    "puppeteer": "^14.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remark-html": "^15.0.1",
    "tailwindcss": "^3.0.18",
    "tasuku": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.12",
    "@types/google.analytics": "0.0.42",
    "@types/node": "^17.0.13",
    "@types/react": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.3.6",
    "esm": "^3.2.25",
    "js-yaml-loader": "^1.2.2",
    "postcss": "^8.4.13",
    "prettier": "^3.0.1",
    "sass": "^1.51.0",
    "stylelint": "^14.8.2",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "typescript": "^5.1.6",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "resolutions": {
    "sass": "1.33.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitormv/vmello-website"
  },
  "bugs": {
    "url": "https://github.com/vitormv/vmello-website/issues"
  },
  "packageManager": "yarn@3.1.1"
}
