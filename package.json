{
  "name": "vmello-website",
  "description": "Source code for Vitor Mello's personal website, built with Next.js and React",
  "author": "Vitor Mello",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=v14.17.0 <=v15.0.0"
  },
  "scripts": {
    "build": "next build",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --cache --cache-location node_modules/.cache/eslint/ .",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:css:fix": "stylelint --fix \"src/**/*.scss\"",
    "export": "yarn build && next export",
    "resume": "yarn export && concurrently --raw --kill-others --success first npm:serve npm:resume:export",
    "resume:export": "node --trace-warnings -r esm cli/export-pdf-resume.js",
    "start": "next dev -p 8091",
    "start:prod": "next -p 8091",
    "serve": "node -r esm static.server.js",
    "test": "yarn lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "next": "^12.0.9",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "puppeteer": "^11.0.0",
    "react": "^18.0.0-rc.0-next-fe905f152-20220107",
    "react-dom": "^18.0.0-rc.0-next-fe905f152-20220107",
    "react-helmet": "^6.1.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "tailwindcss": "^2.2.19",
    "tasuku": "^1.0.2",
    "typeit": "^8.2.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.9",
    "@types/google.analytics": "0.0.42",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.0.9",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "esm": "^3.2.25",
    "js-yaml-loader": "^1.2.2",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "stylelint": "^14.3.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "typescript": "^4.5.5",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "resolutions": {
    "sass": "1.33.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitormv/vmello-website"
  },
  "bugs": {
    "url": "https://github.com/vitormv/vmello-website/issues"
  },
  "packageManager": "yarn@3.1.1"
}
