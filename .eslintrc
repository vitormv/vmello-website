{
    "extends": "airbnb",
    "rules": {
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "react/jsx-indent": [
            2,
            4
        ],
        "react/jsx-filename-extension": [
            0
        ],
        "react/jsx-indent-props": [
            2,
            4
        ],
        "import/prefer-default-export": [
            0
        ]
    },
    "env": {
        "browser": true,
        "jest": true
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "webpack.config.js"
            }
        }
    }
}
